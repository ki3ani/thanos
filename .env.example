# Example environment file
# Copy this file to .env and update the values

# Google Gemini API Key (required for recommendation agent)
GEMINI_API_KEY=your_gemini_api_key_here

# Development settings
DEBUG=true
LOG_LEVEL=debug

# Database settings (if using local database)
DATABASE_URL=postgresql://postgres:password@localhost:5432/boutique

# Redis settings (for caching)
REDIS_URL=redis://localhost:6379

# Service endpoints (for local development)
TOOLBOX_SERVICE_HOST=localhost
TOOLBOX_SERVICE_PORT=8082